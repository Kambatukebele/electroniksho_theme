<ul class="lg:flex lg:justify-start lg:gap-4 lg:items-center">
            {% if settings.social_facebook_link != blank %}          
              <li class="block">
                <a class="block" href="{{ settings.social_facebook_link }}">
                  {% render 'icon-facebook' %}
                </a>
              </li>
            {% endif %}
            {% if settings.social_instagram_link != blank %}
              <li class="block">
                <a class="block" href="{{ settings.social_instagram_link }}">
                  {% render 'icon-instagram' %}
                </a>
              </li>
            {% endif %}
            {% if settings.social_youtube_link != blank %}
              <li>
                <a href="{{ settings.social_youtube_link }}">
                  {% render 'icon-youtube' %}
                </a>
              </li>
            {% endif %}
            {%- if settings.social_tiktok_link != blank -%}
              <li>
                <a href="{{ settings.social_tiktok_link }}">
                  {% render 'icon-tiktok' %}
                </a>
              </li>
            {% endif %}
            {%- if settings.social_twitter_link != blank -%}
              <li>
                <a href="{{ settings.social_twitter_link }}">
                  {% render 'icon-twitter' %}
                </a>
              </li>
            {% endif %}
            {%- if settings.social_snapchat_link != blank -%}
              <li>
                <a href="{{ settings.social_snapchat_link }}">
                  {% render 'icon-snapchat' %}
                </a>
              </li>
            {% endif %}
            {%- if settings.social_pinterest_link != blank -%}
              <li>
                <a href="{{ settings.social_pinterest_link }}">
                  {% render 'icon-pinterest' %}
                </a>
              </li>
            {% endif %}
            {%- if settings.social_tumblr_link != blank -%}
              <li>
                <a href="{{ settings.social_tumblr_link }}">
                  {% render 'icon-tumblr' %}
                </a>
              </li>
            {% endif %}
            {%- if settings.social_vimeo_link != blank -%}
              <li>
                <a href="{{ settings.social_vimeo_link }}">
                  {% render 'icon-vimeo' %}
                </a>
              </li>
            {% endif %}
          </ul>